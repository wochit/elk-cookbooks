{
	"template" : "assets_public*",
	"order" : 0,
	"settings" : {
		"number_of_shards" : 5,
		"number_of_replicas" : 2,
		"analysis": {
            "filter": {
                "wochit_shingle_filter": {
                    "type":             "shingle",
                    "min_shingle_size": 2, 
                    "max_shingle_size": 3, 
                    "output_unigrams":  false   
                },
				"english_stop": {
				  "type":       "stop",
				  "stopwords":  "_english_" 
				},
				"english_stemmer": {
				  "type":       "stemmer",
				  "language":   "english"
				},
				"english_possessive_stemmer": {
				  "type":       "stemmer",
				  "language":   "possessive_english"
				}
            },
            "analyzer": {
                "wochit_shingle_analyzer": {
                    "type":             "custom",
                    "tokenizer":        "standard",
                    "filter": [
                        "lowercase",
                        "wochit_shingle_filter" 
                    ]
                },
				"wochit_english_stemming_analyzer": {
					"tokenizer":  "standard",
				    "filter": [
						"asciifolding",
						"english_possessive_stemmer",
						"lowercase",
						"english_stop",
						"english_stemmer"
					]
					
				}
            }
        }
	},
	"aliases" : {
		"public_assets" : {}
	},
